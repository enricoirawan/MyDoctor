{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": "off",
        "linebreak-style": ["error", "windows"],
        "indent": [2, 2, {"SwitchCase": 1}],
        "react/jsx-filename-extension": [0],
        "no-use-before-define": 0,
        "react/prop-types": 0,
        "no-tabs": "off",
        "no-mixed-spaces-and-tabs": "off"
    }
}
